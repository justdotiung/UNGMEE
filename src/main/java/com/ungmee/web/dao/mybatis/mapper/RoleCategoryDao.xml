<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="ungmee.web.dao.RoleCategoryDao">
 	<select id="get" resultType="ungmee.web.entity.RoleCategory">
	 	SELECT
		    id,
		    name
		FROM
		    role_category
		WHERE 
			ID = #{id}
 	</select>
 
 
 </mapper>